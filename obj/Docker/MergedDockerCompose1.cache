services:
  autofunnerworker:
    build:
      context: C:\work\AutoSalesFunner
      dockerfile: AutoFunnerWorker/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: AutoFunnerWorker
      target: base
    container_name: AutoFunnerWorker
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      ConnectionString: User ID=postgres;Password=qw12cv90;Host=db;Port=54322;Database=test_db;Timeout=30;CommandTimeout=120;
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages;/root/.nuget/fallbackpackages2
    image: auto_funner_worker:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages --additionalProbingPath
        /root/.nuget/fallbackpackages2  "/app/bin/Debug/net5.0/AutoFunnerWorker.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    restart: always
    tty: true
    volumes:
    - C:\work\AutoSalesFunner\AutoFunnerWorker:/app:rw
    - C:\Users\User\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\User\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\User\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Microsoft\Xamarin\NuGet:/root/.nuget/fallbackpackages:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages2:ro
    - C:\Users\User\.nuget\packages:/root/.nuget/packages:ro
    - C:\work\AutoSalesFunner:/src:rw
  db:
    build:
      context: C:\work\AutoSalesFunner
      dockerfile: Infrastructure/DB/Dockerfile
    environment:
      POSTGRES_DB: test_db
      POSTGRES_PASSWORD: qw12cv90
      POSTGRES_USER: postgres
    image: db
    ports:
    - published: 54322
      target: 5432
    restart: always
version: '3.4'